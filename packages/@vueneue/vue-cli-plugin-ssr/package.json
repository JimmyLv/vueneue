{
  "name": "@vueneue/vue-cli-plugin-ssr",
  "version": "0.3.22",
  "description": "Vue CLI 3 plugin to build SSR (Server side rendered) applications",
  "main": "index.js",
  "types": "template/src/vueneue/types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vueneue/vueneue.git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lint:fix": "eslint --ext .js,.vue --fix --ignore-path .eslintignore .",
    "format": "prettier --ignore-path .gitignore --write ./**/*.{js,vue}",
    "prepublishOnly": "cd ui-addon && yarn lint --no-fix && yarn build -- --dest ../ui-addon-dist"
  },
  "keywords": [
    "vue",
    "vuejs",
    "cli",
    "plugin",
    "ssr",
    "vueneue"
  ],
  "author": "Simon Babay <simon@babay.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vueneue/vueneue/issues"
  },
  "homepage": "https://vueneue.github.io/docs/",
  "devDependencies": {
    "@vue/cli-service": "^3.0.0-rc.4",
    "@vue/eslint-config-prettier": "^3.0.0-rc.4",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.7.1",
    "prettier": "^1.13.5",
    "vue-eslint-parser": "^2.0.3"
  },
  "dependencies": {
    "@babel/parser": "^7.0.0-beta.51",
    "chalk": "^2.4.1",
    "ejs": "^2.6.1",
    "fs-extra": "^6.0.1",
    "javascript-stringify": "^1.6.0",
    "micromatch": "^3.1.10",
    "portfinder": "^1.0.13",
    "recast": "^0.15.1",
    "typescript": "^2.9.2"
  }
}
